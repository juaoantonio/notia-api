{
  "name": "notia-api",
  "version": "1.0.0",
  "type": "module",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "src/server.ts",
  "scripts": {
    "dev": "dotenv -e .env -- tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "dotenv -e .env -- node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "prisma:deploy": "dotenv -e .env -- prisma migrate deploy",
    "prisma:studio": "dotenv -e .env -- prisma studio",
    "db:up": "docker compose up -d db || docker compose up -d",
    "db:down": "docker compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/sensible": "^6.0.0",
    "@prisma/client": "^6.13.0",
    "dotenv": "^17.2.1",
    "dotenv-expand": "^12.0.2",
    "fastify": "^5.4.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^5.0.3",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
